# name: test/geoparquet/unsupported.test
# group: [geoparquet]

require parquet

#------------------------------------------------------------------------------
# Test unsupported geometry type
#------------------------------------------------------------------------------
statement ok
COPY (SELECT 'POINT ZM (0 1 2 3)'::GEOMETRY) TO '__TEST_DIR__/t1.parquet' (FORMAT 'parquet');

statement error
COPY (SELECT 'POINT ZM (0 1 2 3)'::GEOMETRY) TO '__TEST_DIR__/t1.parquet' (FORMAT 'parquet', GEOPARQUET_VERSION 'V110');
----
Invalid Input Error: Geoparquet does not support geometries with M coordinates

statement error
COPY (SELECT 'POINT M (0 1 2)'::GEOMETRY) TO '__TEST_DIR__/t1.parquet' (FORMAT 'parquet', GEOPARQUET_VERSION 'V110');
----
Invalid Input Error: Geoparquet does not support geometries with M coordinates