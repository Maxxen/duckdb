query I
SELECT st_makeline(points) 
FROM (
    VALUES 
        ([st_point(1.0, 2.0), st_point(3.0, 4.0)]),
        ([st_point(1.0, 1.0), st_point(2.0, 2.0)]) 
) AS rows(points);
